{
  "name": "lucacracco/robo-drupal8",
  "description": "Robo Tasks for Drupal8 with configurations file.",
  "keywords": [
    "drupal",
    "deployment",
    "automation"
  ],
  "type": "composer-plugin",
  "license": "GPL-2.0-only",
  "authors": [
    {
      "name": "Luca Cracco",
      "email": "craccoluca@gmail.com"
    }
  ],
  "autoload": {
    "psr-4": {
      "Lucacracco\\RoboDrupal8\\": "src"
    }
  },
  "require": {
    "composer-plugin-api": "^1.0.0",
    "composer/installers": "^1.2.0",
    "consolidation/config": "^1.0.0",
    "consolidation/robo": "^1.2.0",
    "cweagans/composer-patches": "^1.6.0",
    "drush/drush": "^9.0.0-beta9",
    "drupal-composer/drupal-scaffold": "^2.2",
    "symfony/yaml": "^3.2.8",
    "grasmash/yaml-cli": "^1.0.0",
    "grasmash/yaml-expander": "^1.2.0",
    "squizlabs/php_codesniffer": "^2.7",
    "wikimedia/composer-merge-plugin": "^1.4",
    "composer/composer": "^1.5"
  },
  "extra": {
    "class": "Lucacracco\\RoboDrupal8\\Composer\\Plugin",
    "branch-alias": {
      "dev-master": "2.x-dev"
    }
  },
  "bin": [
    "bin/rd8"
  ],
  "conflict": {
    "drupal/core": "7.*"
  },
  "suggest": {
    "hirak/prestissimo": "^0.3"
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
