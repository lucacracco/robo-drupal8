{
  "name": "lucacracco/robo-drupal8",
  "description": "Robo Tasks for Drupal8 with configurations file.",
  "keywords": [
    "drupal",
    "deployment",
    "automation"
  ],
  "license": "GPL-2.0-only",
  "authors": [
    {
      "name": "Luca Cracco",
      "email": "craccoluca@gmail.com"
    }
  ],
  "autoload": {
    "psr-4": {
      "Lucacracco\\RoboDrupal8\\": "src"
    }
  },
  "require": {
    "composer-plugin-api": "^1.1",
    "composer/installers": "^1.5",
    "consolidation/config": "^1.0",
    "consolidation/robo": "^1.3",
    "cweagans/composer-patches": "^1.6",
    "drush/drush": "~8.0|^9.3",
    "drupal-composer/drupal-scaffold": "^2.5",
    "symfony/yaml": "^3.2.8",
    "grasmash/yaml-cli": "^1.0.0",
    "grasmash/yaml-expander": "^1.4",
    "squizlabs/php_codesniffer": "^3.3",
    "wikimedia/composer-merge-plugin": "^1.4",
    "composer/composer": "^1.6"
  },
  "extra": {
    "class": "Lucacracco\\RoboDrupal8\\Composer\\Plugin"
  },
  "bin": [
    "bin/rd8"
  ],
  "conflict": {
    "drupal/core": "7.*"
  },
  "suggest": {
    "hirak/prestissimo": "^0.3"
  },
  "minimum-stability": "dev",
  "config": {
    "sort-packages": true
  }
}
