# This file contains default .gitignore rules.

# Ignore directories generated by Composer
example/vendor
example/web/core
example/web/modules/contrib
example/web/themes/contrib
example/web/profiles/contrib

# Ignore configuration files.
example/docker-compose.yml
example/config/default/*.yml
example/web/sites/*/services.yml
example/web/sites/*/settings.php

# Ignore Drupal's file directory
example/web/sites/*/files
example/web/sites/*/private

# Ignore backups folder.
example/build/backups/*.sql
example/build/import-backups/*.sql

# Ignore SimpleTest multi-site environment.
example/web/sites/simpletest

# Ignore files generated by PhpStorm
.settings
.project
.DS_Store
.idea
MAMP*
MAMP_php_error_log_MAMP
